            </div>
          </div>
          </div>
        </div>

        {# Slide 3 #}
        <div class="swiper-slide">
          <div class="swiper-slide__charts">
            <div class="swiper-slide__charts--img zaux-bg-default-white shadow-chart mx-1 mb-2 mb-lg-4 py-6 py-lg-10 px-4 px-lg-8">

                {{
                  Img.content({
                  "isLazy": false,
                  "src": "media/Doctor.png",
                  "src2x": "media/Doctor.png",
                  "width": 700,
                  "height": 1050,
                  "className":  ["zaux-fit-contain position-absolute top-0 start-0 w-100 h-100 -in-lazy -is-loaded"],
                  "title": "title",
                  "alt": "alt",
                  "wrapperTag": ["picture"],
                  "wrapperClassName": ["c-img d-block h-100 position-relative c-ratio--chart c-img--lazy -is-loaded"]
                  })
                }}
            </div>

