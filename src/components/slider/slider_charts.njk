            </div>
          </div>

          </div>
        </div>

      </div>

    </div>
    <!-- .c-slider__instance -->
  </div>

  <div class="swiper-pagination"></div>
  <div class="swiper-navigation">
        <div class="chart-swiper-button-prev"> {{
                    Icon.content({
                      "className": "",
                      "iconName": "icozaux1-arrowL",
                      "iconSvgUrl": app.fonts.icozaux1.url
                      })
                    }}
        </div>
        <div class="chart-swiper-button-next">
          {{
              Icon.content({
                "className": "",
                "iconName": "icozaux1-arrowR",
